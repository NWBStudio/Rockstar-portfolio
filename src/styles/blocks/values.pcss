.values {
    position: relative;
    background-color: $white;
    padding: 135px 0 145px 0;
}

.values__title {
    font-size: 30px;
    line-height: 36px;
    font-weight: $bold;
    color: $font-dark-purple;
}

.values__info {
    font-weight: $semibold;
    color: $font-dark-purple;
    opacity: 0.5;
}

.values__skills {
    display: flex;
    flex-direction: column;
}